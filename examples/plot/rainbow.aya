.# Draws a rainbow in a plot window

import ::plot

plot.plot!:p;
[0.857 1.1] p.y.:lim;
16 p.:stroke;
0 p.:legend;
"A Rainbow" p.:title;
0 p.y.:visible;
0 p.x.:visible;
[ "violet" "indigo" "blue" "green" "yellow" "orange" "red"] p.:color_cycle;

[0.9 :1p .9 - 600] .R :x;

7R :# {i,
    x {:i^, Ms i 0.011* +} :{} p.plot
};

p.view
