.# The 100 Doors Problem

.# initialize doors
0 100L:doors;

.# do 100 passes
[100,] # {pass,
  [pass $$+ 100,] # {door,
    .# Swap the state of the door
    doors.[doorV] ! doors.:[doorV];
  }
};

.# print the doors
doors E.R :# {door,
  doors.[door] {"door $(doorB) is open" :P} ?
};
